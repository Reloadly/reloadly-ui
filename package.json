{
    "name": "reloadly-ui",
    "version": "0.0.1-beta.20",
    "description": "Angular UI library",
    "repository": {
        "type": "git",
        "url": "https://github.com/Reloadly/reloadly-ui.git"
    },
    "scripts": {
        "build:dev": "ng build reloadly-ui --configuration=development --watch",
        "build:scss": "node-sass --include-path scss src/styles/styles.scss dist/reloadly-ui/styles/styles.css",
        "build:prod": "ng build reloadly-ui --configuration=production && npm run build:scss",
        "npm:publish:beta": "cd dist/reloadly-ui && npm publish --tag beta",
        "npm:publish:latest": "cd dist/reloadly-ui && npm publish --tag latest"
    },
    "peerDependencies": {
        "@angular/common": "^16.1.8",
        "@angular/core": "^16.1.8"
    },
    "devDependencies": {
        "@angular/animations": "^16.1.8",
        "@angular/common": "^16.1.8",
        "@angular/compiler": "^16.1.8",
        "@angular/core": "^16.1.8",
        "@angular/forms": "^16.1.8",
        "@angular/platform-browser": "^16.1.8",
        "@angular/platform-browser-dynamic": "^16.1.8",
        "@angular/router": "^16.1.8",
        "zone.js": "~0.13.1",
        "@angular-devkit/build-angular": "^16.2.0",
        "@angular/cli": "~16.2.0",
        "@angular/compiler-cli": "^16.2.1",
        "@types/jasmine": "~4.3.5",
        "jasmine-core": "~5.1.0",
        "karma": "~6.4.2",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-packagr": "^16.2.0",
        "node-sass": "^9.0.0",
        "nodemon": "^3.0.1",
        "tslib": "^2.6.1",
        "typescript": "~5.1.6"
    },
    "keywords": [
        "angular",
        "ui",
        "components",
        "accordion",
        "alert",
        "buttons",
        "datepicker",
        "dropdown",
        "modal",
        "offcanvas",
        "pagination",
        "progressbar",
        "table",
        "timepicker"
    ],
    "private": false,
    "sideEffects": false,
    "license": "MIT",
    "author": "Reloadly",
    "bugs": {
        "url": "https://github.com/Reloadly/reloadly-ui/issues"
    },
    "homepage": "https://github.com/Reloadly/reloadly-ui#readme"
}
